%{
#include<stdio.h>
%}

%%

("if")|("else")|("while")|("do")|("break")|("continue")|("case")|("sizeof")|("typedef")|("switch")|("return") {printf("Keyword ");}
("int")|("char")|("double")|("float")|("long")|("short")|("unsigned")|("void")|("static")|("struct")|("goto") {printf("Keyword ");}
[*][_a-zA-Z][a-zA-Z0-9]* {printf("Pointer ");}
[_a-zA-Z][a-zA-Z0-9]*("[")[0-9]*("]") {printf("Array ");}
[_a-zA-Z][a-zA-Z0-9]* {printf("Identifier ");}
[0-9]* {printf("Number ");}
[0-9]*"."[0-9]+ {printf("Number ");} 
[+][0-9] {printf("Number ");}
[-][0-9] {printf("Number ");}
"@"|"*"|";"|"'"|","|"%"|"$"|"#"	{printf("Operator ");}
"+"|"-"|"*"|"/"|"!"|"&"|"|"|"^"|"="|"<"|">"  {printf("SpecialCharacter ");}
%%

